<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>RunAtLoad</key>
	<true/>
	<key>ProgramArguments</key>
	<array>
		<string>/Library/Application Support/MICE/360Daemon.app/Contents/MacOS/360Daemon</string>
	</array>
	<key>Sockets</key>
	<dict>
		<key>Listeners</key>
		<array>
			<dict>
				<key>SockFamily</key>
				<string>IPv4</string>
				<key>SockServiceName</key>
				<string>360Daemon</string>
				<key>SockType</key>
				<string>stream</string>
			</dict>
		</array>
	</dict>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>com.mice.360Daemon</string>
</dict>
</plist>
